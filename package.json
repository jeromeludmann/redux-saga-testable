{
  "name": "redux-saga-testable",
  "version": "0.2.2",
  "description": "Practical unit test library for redux-saga",
  "main": "lib/runner.js",
  "types": "lib/runner.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "jest",
    "cover": "npm test -- --collectCoverage",
