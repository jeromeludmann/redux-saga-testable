{
  "name": "redux-saga-testable",
  "version": "0.3.1",
  "description": "Make redux-saga more easily testable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "type-checking": "tsc --noEmit",
    "circular-deps": "madge --circular src",
    "test-coverage": "npm test -- --coverage",
    "checks": "npm run type-checking && npm run circular-deps && npm run test-coverage",
    "prebuild": "rm -rf lib",
    "prepublishOnly": "npm run checks && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeromeludmann/redux-saga-testable.git"
  },
  "keywords": [
    "redux-saga",
    "test",
    "testing",
    "redux",
    "saga"
  ],
  "author": {
    "name": "Jerome Ludmann",
    "email": "jerome.ludmann@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeromeludmann/redux-saga-testable/issues"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "devDependencies": {
    "@redux-saga/testing-utils": "^1.1.3",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "coveralls": "^3.0.9",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "madge": "^3.6.0",
    "prettier": "^1.19.1",
    "redux-saga": "^1.1.3",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "redux": "^4.0.0",
    "redux-saga": "^1.0.0"
  },
  "dependencies": {}
}
